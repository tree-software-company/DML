number age = 27;
assert age > 0;

string name = "Szymon";
assert name != "";

function add(a, b) {
    return a + b;
}

function multiply(a, b) {
    return a * b;
}

function square(x) {
    return multiply(x, x);
}

number PI = 3.14159;

regex emailPattern = "[a-zA-Z0-9._%+-]+@[a-zA-Z0-9.-]+\\.[a-z]{2,}";
string userEmail = "test@example.com";
validate userEmail matches emailPattern;
